<div class="row">
	<div class="col-md-4 col-md-offset-4">
		<%= will_paginate%>
	</div>
</div>

<% @chefs.each do |chef| %>
	<div class="row">
	<div class="col-md-4 col-md-offset-4">
		<%= image_tag (chef.picture.url) if chef.picture? %>
		<p>Chef Name: <%= chef.chef_name%></p>
	</div>
</div>
<% end %>